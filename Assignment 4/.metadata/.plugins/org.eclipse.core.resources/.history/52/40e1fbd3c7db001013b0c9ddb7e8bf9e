#include "gpio.h"

#if !defined(_SOFT_FP) && defined(_ARM_FP)
  #warning "FPU is not initialized, but the project is compiling for an FPU. Please initialize the FPU before use."
#endif



int main(void)
{
	led_init();
	switch_init();
	while(1)
	{
		led_off();
		while(switch() == 1)
				{
					led_on();
				}

	}
}
